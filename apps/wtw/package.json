{
	"name": "@dills1220/wtw",
	"version": "0.0.2",
	"description": "What to watch, Unoffical Justwatch api wrapper",
	"scripts": {
		"test": "mocha -r ts-node/register \"*.spec.ts\"",
		"build": "tsc -p .",
		"watch": "tsc -p . --watch",
		"compile": "tsc",
		"clean": "tsc --build --clean && rm -rf ./types",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"prepublishOnly": "npm run build",
		"version": "auto-changelog -p && git add HISTORY.md"
	},
	"dependencies": {
		"got": "11.8.2",
		"lodash": "^4.17.21",
		"rxjs": "7.5.5",
		"camelcase-keys": "~8.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dills122/where-my-cage-at/"
	},
	"bugs": {
		"url": "https://github.com/dills122/where-my-cage-at/issues"
	},
	"devDependencies": {
		"@types/chai": "~4.3.0",
		"@types/lodash": "^4.14.172",
		"@types/mocha": "~9.1.0",
		"@types/node": "^18.6.5",
		"@types/sinon": "~10.0.2",
		"@typescript-eslint/eslint-plugin": "5.28.0",
		"@typescript-eslint/parser": "5.28.0",
		"eslint": "~8.17.0",
		"chai": "~4.3.4",
		"typescript": "~4.7.2",
		"lint-staged": "^13.0.0",
		"mocha": "~10.0.0",
		"sinon": "~14.0.0",
		"ts-node": "~10.8.0"
	},
	"lint-staged": {
		"*.ts": [
			"prettier --write",
			"npm run lint:fix"
		],
		"package.json": "prettier --write",
		"*.md": "prettier --write"
	},
	"keywords": [
		"justwatch",
		"just-watch",
		"streaming-availability",
		"api-wrapper",
		"streaming-services"
	],
	"author": {
		"name": "Dylan Steele",
		"email": "dylansteele57@gmail.com"
	},
	"license": "ISC"
}
